<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Custom ContactUs Form Page</argument>
            </action>

            <referenceContainer name="content">
            <block class="SimplifiedMagento\RequestFlow\Block\Booking" name="contactus.Contactus" template="SimplifiedMagento_RequestFlow::contactUs.phtml"/>
            </referenceContainer>

        </referenceBlock>
    </body>
    <head>
         <css src="SimplifiedMagento_RequestFlow::css/custom.css" rel="stylesheet" type="text/css"  />
    </head>
</page>

